{
	"name": "not-error",
	"version": "0.0.20",
	"description": "not-* family error in not- environment",
	"main": "index.js",
	"bin": {
		"not-error": "./bin/not-error.js"
	},
	"scripts": {
		"build": "./bin/build.sh",
		"buildtest": "./bin/build.sh --key=test.key --url-node=/node/api --url-browser=/browser/api",
		"prelint": "npm run buildtest",
		"lint": "./node_modules/.bin/eslint ./src/**/**",
		"pretest": "npm run lint",
		"test": "_ENV_NODE_='liquid' NOT_NODE_ERROR_URL_NODE='http://reporter.local/api/key/collect' NOT_NODE_ERROR_KEY='error@reporter.local' ./node_modules/.bin/mocha --reporter spec ./test/node && _ENV_NODE_='liquid' electron-mocha  --renderer --reporter spec ./test/browser",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"cover": "_ENV_NODE_='liquid' nyc npm run test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-error.git"
	},
	"keywords": [
		"not-framework",
		"not-node",
		"error"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-error/issues"
	},
	"homepage": "https://github.com/interrupter/not-error#readme",
	"dependencies": {
		"ejs": "^2.6.1"
	},
	"devDependencies": {
		"electron": "^3.0.7",
		"electron-mocha": "^6.0.4",
		"not-config": "*",
		"@babel/core": "*",
		"babel-env": "*",
		"babel-polyfill": "*",
		"babel-preset-es2015-rollup": "*",
		"chai": "^4.2.0",
		"chai-as-promised": "*",
		"eslint": "^5.8.0",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.5.5",
		"mocha": "^5.2.0",
		"nyc": "*",
		"request": "^2.88.0",
		"rollup": "^0.66.6",
		"rollup-plugin-terser": "^3.0.0",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-eslint": "^5.0.0",
		"rollup-plugin-filesize": "^5.0.1"
	},
	"nyc": {
				"include": [
						"index.js",
						"src/**/**/**.js"
				],
				"exclude": [
						"tests/**/**/**/**.js"
				],
				"reporter": [
						"html"
				],
				"check-coverage": true,
				"per-file": true,
				"lines": 95,
				"statements": 95,
				"functions": 95,
				"branches": 95,
				"watermarks": {
						"lines": [
								80,
								95
						],
						"functions": [
								80,
								95
						],
						"branches": [
								80,
								95
						],
						"statements": [
								80,
								95
						]
				}
		}
}

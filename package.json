{
	"name": "not-error",
	"version": "0.0.4",
	"description": "not-* family error in not- environment",
	"main": "index.js",
	"scripts": {
		"test": "_ENV_NODE_='liquid' ./node_modules/.bin/mocha --reporter spec",
		"pretest": "./node_modules/.bin/eslint ./src/**/**",
		"lint": "./node_modules/.bin/eslint ./src/**/**",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"build": "./node_modules/.bin/rollup -c",
		"cover": "_ENV_NODE_='liquid' istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-error.git"
	},
	"keywords": [
		"not-framework",
		"not-node",
		"error"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-error/issues"
	},
	"homepage": "https://github.com/interrupter/not-error#readme",
	"dependencies": {
		"not-config": "0.0.3"
	},
	"devDependencies": {
		"babel-core": "*",
		"babel-env": "*",
		"babel-polyfill": "*",
		"babel-preset-es2015-rollup": "*",
		"chai": "*",
		"chai-as-promised": "*",
		"cssnano": "^3.10.0",
		"eslint": "*",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.5.5",
		"mocha": "*",
		"postcss-cssnext": "*",
		"postcss-env-replace": "*",
		"postcss-nested": "*",
		"postcss-simple-vars": "*",
		"request": "*",
		"rollup": "*",
		"rollup-plugin-babel": "*",
		"rollup-plugin-commonjs": "*",
		"rollup-plugin-eslint": "*",
		"rollup-plugin-filesize": "*",
		"rollup-plugin-node-resolve": "*",
		"rollup-plugin-postcss": "*",
		"rollup-plugin-replace": "*",
		"rollup-plugin-sass": "*",
		"rollup-plugin-uglify": "*",
		"rollup-watch": "*"
	}
}

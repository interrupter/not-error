{
	"name": "not-error",
	"version": "0.0.8",
	"description": "not-* family error in not- environment",
	"main": "index.js",
	"bin": {
		"not-error": "./bin/not-error.js"
	},
	"scripts": {
		"build": "./bin/build.sh",
		"prelint": "npm run build",
		"lint": "./node_modules/.bin/eslint ./src/**/**",
		"pretest": "npm run lint",
		"test": "_ENV_NODE_='liquid' ./node_modules/.bin/mocha --reporter spec ./test/node && _ENV_NODE_='liquid' electron-mocha  --renderer --reporter spec ./test/browser",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"cover": "_ENV_NODE_='liquid' istanbul cover _mocha $(find ./test/node -name \"*.js\" -not -path \"./node_modules/*\")"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-error.git"
	},
	"keywords": [
		"not-framework",
		"not-node",
		"error"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-error/issues"
	},
	"homepage": "https://github.com/interrupter/not-error#readme",
	"dependencies": {
		"ejs": "^2.6.1"
	},
	"devDependencies": {
		"electron": "^3.0.7",
		"electron-mocha": "^6.0.4",
		"not-config": "*",
		"@babel/core": "*",
		"babel-env": "*",
		"babel-polyfill": "*",
		"babel-preset-es2015-rollup": "*",
		"chai": "^4.2.0",
		"chai-as-promised": "*",
		"eslint": "^5.8.0",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.5.5",
		"mocha": "^5.2.0",
		"request": "^2.88.0",
		"rollup": "^0.66.6",
		"rollup-plugin-terser": "^3.0.0",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-eslint": "^5.0.0",
		"rollup-plugin-filesize": "^5.0.1"
	}
}
